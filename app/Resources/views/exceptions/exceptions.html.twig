{% extends '::base.html.twig' %}
{#http://symfonybricks.com/en/brick/custom-exception-page-404-not-found-and-other-exceptions#}

{% block content %}
  <header class="masthead text-white text-center">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-xl-9 mx-auto">
          <h1 class="mb-5">
            {{ 'error.message'|trans({'%error%': exception.statusCode}) }}
          </h1>
        </div>
        <div class="col-md-10 col-lg-8 col-xl-7 mx-auto">
          {{ dump(exception) }}
          {#{% include ':forms:homesearch.html.twig' %}#}
        </div>
      </div>
    </div>
  </header>
{% endblock %}